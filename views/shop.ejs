  <body class="text-center">
    
<main class="form-signin">
  <form action="/shop" method="POST">
    <img class="mb-1" src= "Logo.png" alt="" width="176" height="169">
    <h1 class="display">IND-Stock</h1>
    <h1 class="h3 mb-3 fw-normal">Shop Registration</h1>

    <div class="form-floating">
      <input type="text" class="form-control" id="floatingInput" placeholder="ABC" name="shopName">
      <label for="floatingInput">Shop Name</label>
    </div>

    <div class="form-floating">
        <input type="text" class="form-control" id="floatingInput" placeholder="name@example.com" name="shopAddress">
        <label for="floatingInput">Shop Address</label>
      </div>

      <div class="form-floating">
        <input type="email" class="form-control" id="floatingInput" placeholder="name@example.com" name="shopEmail">
        <label for="floatingInput">Email address</label>
      </div>
      
      <div class="form-floating">
        <input type="password" class="form-control" id="floatingInput" placeholder="name@example.com" name="shopPassword">
        <label for="floatingInput">Password</label>
      </div>
      
      <div class="form-floating">
        <input type="number" class="form-control" id="floatingInput" placeholder="name@example.com" name="shopNumber">
        <label for="floatingInput">Phone Number</label>
      </div>
      
    <select class="selectpicker" data-style="btn-info" name="locality" class="btn btn-secondary dropdown-toggle btn-lg btn-warning">
      <optgroup label="Select Locality">
          <option name="" value="0">Select Locality</option>
          <option name="table1" value="central">Central Delhi</option>
          <option name="table2" value="north">North Delhi</option>
          <option name="table3" value="south">South Delhi</option>
          <option name="table3" value="east">East Delhi</option>
          <option name="table3" value="west">West Delhi</option>
      </optgroup>
  </select>
  
  <editable-table>
    <table>
      <thead>
        <tr>
          <th>Product</th>
          <th>Stock</th>
        </tr>
      </thead>
      <tbody>
          <tr>
          <td>
            <input name="product" placeholder="New Product" autocomplete="off"/>
          </td>
          <td>
            <input name="stock" placeholder="Units" type="text" autocomplete="off"/>
          </td>
          <td>
            <span data-remove>Ã—</span>
          </td>
        </tr>
      </tbody>
    </table>
  </editable-table>

    <button class="w-100 btn btn-lg btn-primary submit-btn" type="submit"><i class="fas fa-shopping-cart"></i> Register Shop</button>    
  </form>



  <p class="mt-5 mb-3 text-muted">&copy; Interm-Ventors</p>

  <p class="lead">Not a shop? Sign up <span class="login-span"><a href="/register">here </a></span></p>



